<div class="dtprofile-background">

	<div class="container">
	 <div class="row">
		<div class="col-md-12">
			<div class="dtprofile-contents">
				<div class="row">
					<div id="dtprofile-icon"></div>
					<div class="dtprofile-title">PROFILE</div>
					<div id="dtprofile-closebtn"><a href="/chatPage">X</a></div>
				</div>
				<div class="row">	
					<div class="dtprofile-image">
						<!-- <div class="dtprofile-onoff"></div> -->
						<div class="dtprofile-name"><%= user.name %></div>
					</div>	
					<div class="dtprofile-text">
						<div class="dt-title">TITLE</div>
						<div class="dt-text"><%= user.position %></div>
						<div class="dt-title">COMPANY</div>
						<div class="dt-text"><%= user.company %></div>
						<div class="dt-title">EMAIL Address</div>
						<div class="dt-text"><%= user.email %></div>
						<div class="dt-title">Phone Number</div>
						<div class="dt-text"><%= user.phoneNumber %></div>
  
					</div>
				</div>	
			</div>
			
		<div class="dtprofile-setting">
			<div class="row">
				<div id="dtsetting-icon"></div>
				<div class="dtprofile-title">Account Setting</div>
			</div>

			<div class="setting-title">E-mail Address<button class="setting-close emailcl">CLOSE</button></div>
			 
			<div class="setting-line"></div>
			<div class="setting-text email">
				<div class="inputtext">Current E-mail Address</div>
				<div class="inputform">
				  <input type="text" class="form-control" id="emailAdress">
				</div>
				<div class="inputtext">New E-mail Address</div>
				<div class="inputform">
				  <input type="text" class="form-control" id="emailAdress">
				</div>
				<button class="account-message" id="#">Update E-mail</button>
			</div>
			
			<div class="setting-title">Phone Number<button class="setting-close phcl">CLOSE</button></div>
			<div class="setting-line"></div>
			<div class="setting-text ph">

				<div class="inputtext">Current Phone number</div>
					<div class="inputform">
					  <select class="form-control" id="emailPhone"> 
					  <option>+82</option>
					  <option>+01</option> 
					  <option>+81</option> 
					  <option>+86</option> 
					  <option>+61</option> 
					  <option>+44</option>
					  <option>+63</option> 
					  <option>+852</option> 
					  <option>+66</option> 		
					  </select>
					  <input type="text" class="form-control" id="phoneNumber">
					</div>
				<div class="inputtext">New Phone number</div>
					<div class="inputform">
					  <select class="form-control" id="emailPhone"> 
					  <option>+82</option>
					  <option>+01</option> 
					  <option>+81</option> 
					  <option>+86</option> 
					  <option>+61</option> 
					  <option>+44</option>
					  <option>+63</option> 
					  <option>+852</option> 
					  <option>+66</option> 					  
					  </select>
					  <input type="text" class="form-control" id="phoneNumber">
					</div>
						<button class="account-message" id="#">Update Phone</button>
			</div>
			
			
			<div class="setting-title">Password<button class="setting-close pwcl">CLOSE</button></div>
			<div class="setting-line"></div>
			<div class="setting-text pw">

				<div class="inputtext">Current E-mail Address</div>
					<div class="inputform">
					  <input type="text" class="form-control" id="emailAdress">
					</div>
				<div class="inputtext">New E-mail Address</div>
					<div class="inputform">
					  <input type="text" class="form-control" id="emailAdress">
					</div>
						<button class="account-message" id="<%= user.email %>">Save Password</button>
			</div>
			
			<div class="setting-signout">Sign Out</div>
			<div class="setting-line"></div>
			<div class="setting-text">

				<div class="inputtext">Confirm your password</div>
				<div class="inputform">
				  <input type="text" class="form-control" id="emailAdress">
				</div>
				<button class="account-signout" id="<%= user.email %>">Sign out</button>
			</div>
		</div>
		</div>	
	  </div>
	 </div>
	</div>	

<body>
 	<%
 		data.forEach(function(log, index) {
 			if(log.rece_id.email == user){
 			if(search == null || log.send_id.name.indexOf(search)>-1 || log.send_id.company.indexOf(search)>-1 ||log.send_id.position.indexOf(search)>-1 || log.send_id.email.indexOf(search)>-1 || log.name.indexOf(search)>-1){
 				if(category == 'GALLERY' && log.rtype == 'image'){
 	%>
				  	<div class="gallery-div filebox">
			  			<div class="url" >
			  				<img class="url-image" alt="<%= log.name %>" src="<%= log.url %>">
			  			</div>
						<!-- css수정  -->
						<div class="textbox">
			  			<div class="file-name"><%= log.name %></div>
			  			<div class="from-name">From : <%= log.send_id.name %></div>
				        <div class="file-date"><%= moment(log.date).format('YY.MM.DD [(]dd[)] h:mm a') %></div>
			  			</div>
			  		</div>
 	
 	<%
 				}else if(category == 'ARCHIVE' && log.rtype != 'image'){

 	%>
 	 							<div class="filebox">
 							 <!--css ppt icon  -->
				  			<% if(log.type == 'pptx'){ %>
				  			<div class="url" >
				  				<div class="pptx-image"></div>
				  			</div>
				  			<% } %>
				  			<!--css xlsx icon  -->
				  			<% if(log.type == 'xlsx'){ %>
				  			<div class="url" >
				  				<div class="xlsx-image"></div>
				  			</div>
				  			<% } %>
				  			<!--css pdf icon  -->
				  			<% if(log.type == 'pdf'){ %>
				  			<div class="url" >
				  				<div class="pdf-image"></div>
				  			</div>
				  			<% } %>
				  			<!--css doc icon  -->
				  			<% if(log.type == 'docx'){ %>
				  			<div class="url" >
				  				<div class="docx-image"></div>
				  			</div>
				  			<% } %>
				  			<!--css doc icon  -->
				  			<% if(log.type == 'zip'){ %>
				  			<div class="url" >
				  				<div class="zip-image"></div>
				  			</div>
				  			<% } %>
			  		<div class="textbox">
			  			<!-- css수정  -->
			  			<div class="file-name"><%= log.name %></div>
			  			<div class="from-name">From : <%= log.send_id.name %></div>
				        <div class="file-date"><%= moment(log.date).format('YY.MM.DD [(]dd[)] h:mm a') %></div>
<!-- 			  			<a type="button" href="<%= log.url %>" style="float:right;">Down</a> -->
			  		</div>
			  		</div>
			  		
	<%
				}else if(category == 'ALL'){
	%>
			  		<div class="filebox">
			  			<% if(log.rtype == 'image'){ %>
			  			<div class="url" >
			  				<img class="url-image" alt="<%= log.name %>" src="<%= log.url %>">
			  			</div>
			  			<% } %>
			  				<!--css ppt icon  -->
				  			<% if(log.type == 'pptx'){ %>
				  			<div class="url" >
				  				<div class="pptx-image"></div>
				  			</div>
				  			<% } %>
				  			<!--css xlsx icon  -->
				  			<% if(log.type == 'xlsx'){ %>
				  			<div class="url" >
				  				<div class="xlsx-image"></div>
				  			</div>
				  			<% } %>
				  			<!--css pdf icon  -->
				  			<% if(log.type == 'pdf'){ %>
				  			<div class="url" >
				  				<div class="pdf-image"></div>
				  			</div>
				  			<% } %>
				  			<!--css doc icon  -->
				  			<% if(log.type == 'docx'){ %>
				  			<div class="url" >
				  				<div class="docx-image"></div>
				  			</div>
				  			<% } %>
				  			<!--css doc icon  -->
				  			<% if(log.type == 'zip'){ %>
				  			<div class="url" >
				  				<div class="zip-image"></div>
				  			</div>
				  			<% } %>
			  			<!-- css수정  -->
			  			<div class="textbox">
			  			<div class="file-name"><%= log.name %></div>
			  			<div class="from-name">From : <%= log.send_id.name %></div>
				        <div class="file-date"><%= moment(log.date).format('YY.MM.DD [(]dd[)] h:mm a') %></div>
			  			</div>
			  			<!-- <a type="button" href="<%= log.url %>" style="float:right;">Down</a> -->
			  		</div>
			  		
 	<%			}
 				}
 			}
 		});
 	%>
</body>
<body>
<div id="relationResult">
<div id="top-side">
<div id="top-result">Search Result</div>
<%
		var chk=1;
		friends.forEach(function(friends, index) {
			user.friends.forEach(function(user, index){
				if(friends.email == user.friend.email){
					chk=0;
				}
			});
			if(friends.email != me){
%>				
				<div id="topcontent"></div>
				<!-- ë‚˜ : <%= me %> -->
				<div id="<%= friends.email %>"  class='has-sub active'>
					<div id ="connect-profile" href='#'>
					   <div id="connect-img" style="background-image:url(<%=friends.user_img%>), url(https://s3-ap-northeast-1.amazonaws.com/archa-bucket/user-profile/background1.png);"> </div>
						<div id="connect-name" class="<%= friends.name %>"><%= friends.name %><br/>
							<div id="connect-position" class="<%= friends.position %>"><%= friends.position %></div>
							<div id="connect-company" class="<%= friends.company %>"><%= friends.company %></div> 
							<br/>
						</div>
						<% if(chk){%>
						<button type="button" id="<%= friends.email %>" class="addRelation">Request</button>
						<% } %>							
					</div>
				</div>
				
<%			}
			chk=1;
		});
%>
</div>
</div>
</body>
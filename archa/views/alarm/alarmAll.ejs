<body>
    <div role="tabpanel">
		<ul class="nav nav-tabs nav-justified" role="tablist" id="tabalarm">
		  <li role="presentation" id="tabReq" class="altab active"><a href="#request" aria-controls="#request" role="tab" data-toggle="tab">Request</a></li>
		  <li role="presentation" id="tabNoti" class="altab"><a href="#notification" aria-controls="#notification" role="tab" data-toggle="tab">Notification</a></li>
		</ul>
		
		<div class="tab-content">
			<div role="tabpanel" class="arch-tab tab-pane active" style="width: 250px; " id="request">
				<%
					if(req){
						req.forEach(function(req, index) { 
				%>
				<div class="request" id="<%= req.email %>">
					<div class="fleft">
						 <%= req.name %>
						 <%= req.position %>
						 <%= req.company %>
				 	</div>
					<button type="button" id="<%= req.email %>" class="conn fleft">Connect</button>
					<button type="button" id="<%= req.email %>" class="dec">Decline</button>
				</div>
				<%
						});
					}
				%>
			</div>
			<div role="tabpanel" class="arch-tab tab-pane" id="notification">
				<%
					if(his){
						his.forEach(function(his, index) { 
				%>
				 <%= his.name %><br/>
				<%	
						});
					}
				%>
			</div>
			<button type="button" id="findRelation" style="width: 100%;">Find New Relation</button>
		</div>
	</div>
</body>